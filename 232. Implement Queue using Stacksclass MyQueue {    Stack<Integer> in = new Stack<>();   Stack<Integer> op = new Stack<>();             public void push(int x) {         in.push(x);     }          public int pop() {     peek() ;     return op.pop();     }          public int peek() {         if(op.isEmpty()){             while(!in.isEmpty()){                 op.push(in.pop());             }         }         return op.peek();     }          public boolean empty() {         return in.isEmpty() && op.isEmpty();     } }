class MyQueue {

  Stack<Integer> in = new Stack<>();
  Stack<Integer> op = new Stack<>();
  
    
    public void push(int x) {
        in.push(x);
    }
    
    public int pop() {
    peek() ;
    return op.pop();
    }
    
    public int peek() {
        if(op.isEmpty()){
            while(!in.isEmpty()){
                op.push(in.pop());
            }
        }
        return op.peek();
    }
    
    public boolean empty() {
        return in.isEmpty() && op.isEmpty();
    }
}
