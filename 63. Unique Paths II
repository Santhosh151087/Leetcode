class Solution {
    public int uniquePathsWithObstacles(int[][] mat) {
        
        int r = mat.length;
        int c = mat[0].length;
        if(mat[0][0]==1 || mat[r-1][c-1]==1)
        return 0;
        int [][]ans = new int [r][c];
        boolean isTrue = false;
        for(int i=0;i<c;i++){
            if(mat[0][i]==1 || isTrue){
                ans[0][i] = 0;
                isTrue = true;
            }
            
            else if(!isTrue)
            ans[0][i] = 1;
        }
        isTrue = false;
        for(int i=0;i<r;i++){
            if(mat[i][0]==1 || isTrue){
                 ans[i][0] = 0;
                 isTrue = true;
            }
           
            else if(!isTrue)
            ans[i][0]=1;
        }
        for(int i=1;i<r;i++){
            for(int j=1;j<c;j++){
                if(mat[i][j]==1){
                    ans[i][j] = 0;
                }
                else{
                    ans[i][j] = ans[i-1][j] + ans[i][j-1];
                }
            }
        }
       
        return ans[r-1][c-1];
    }
}
