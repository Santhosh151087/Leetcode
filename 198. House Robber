class Solution {
    public int rob(int[] nums) {
        int len = nums.length;
        if(len==1)
        return nums[0];
        
        int [] ans = new int [len];
        ans[0] = nums[0];
        ans[1] = Math.max(nums[0],nums[1]);
        for(int i=2;i<len;i++){
            ans[i] = Math.max(ans[i-1],ans[i-2]+nums[i]);
        }
        return ans[len-1];

    }
}
